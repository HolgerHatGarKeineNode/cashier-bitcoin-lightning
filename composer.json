{
    "name": "HolgerHatGarKeineNode/cashier-bitcoin-lightning",
    "description": "Cashier Bitcoin Lightning provides an expressive, fluent interface to Lnbits's subscription billing services.",
    "keywords": ["laravel", "bitcoin", "lightning", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/HolgerHatGarKeineNode/cashier-bitcoin-lightning/issues",
        "source": "https://github.com/HolgerHatGarKeineNode/cashier-bitcoin-lightning"
    },
    "authors": [
        {
            "name": "HolgerHatGarKeineNode",
            "email": "fsociety.mkv@pm.me"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/contracts": "^7.9|^8.0|^9.0|^10.0",
        "illuminate/database": "^7.9|^8.0|^9.0|^10.0",
        "illuminate/http": "^7.9|^8.0|^9.0|^10.0",
        "illuminate/routing": "^7.9|^8.0|^9.0|^10.0",
        "illuminate/support": "^7.9|^8.0|^9.0|^10.0",
        "illuminate/view": "^7.9|^8.0|^9.0|^10.0",
        "moneyphp/money": "^3.2|^4.0",
        "nesbot/carbon": "^2.0",
        "spatie/url": "^1.3.5|^2.0",
        "symfony/http-kernel": "^5.0|^6.0",
        "symfony/polyfill-intl-icu": "^1.22.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Bitcoin\\Lightning\\Lnbits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Bitcoin\\Lightning\\Lnbits\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
